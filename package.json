{
  "name": "@glitchybyte/babel-plugin-glog",
  "version": "0.2.0",
  "author": "GlitchyByte",
  "license": "Apache-2.0",
  "description": "Babel plugin for enhancing glog (a console logger replacement) with source filenames and line numbers while removing all logs in production.",
  "keywords": [
    "babel-plugin",
    "console",
    "log",
    "source",
    "line"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/GlitchyByte/babel-plugin-glog"
  },
  "files": [
    "glog",
    "src/plugin"
  ],
  "main": "./src/plugin/plugin.js",
  "exports": {
    ".": "./src/plugin/plugin.js",
    "./glog": {
      "import": "./glog/index.js",
      "types": "./glog/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "glog": [
        "./glog/index.d.ts"
      ]
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsc"
  },
  "devDependencies": {
    "typescript": "^5.8.2"
  }
}
